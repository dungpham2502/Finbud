ion class=\"transactions\">\n      <h2>Daily Transactions</h2>\n      <div class=\"transaction-container\">\n        <div class=\"transaction-box\">\n          <input type=\"text\" placeholder=\"Description\" v-model=\"transaction.description\">\n          <input type=\"number\" placeholder=\"Amount\" v-model=\"transaction.amount\">\n          <button @click=\"addTransaction\">Add Transaction</button>\n        </div>\n        <div class=\"transaction-list\">\n          <table>\n            <thead>\n              <tr>\n                <th>Date</th>\n                <th>Description</th>\n                <th>Amount</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-for=\"trans in transactions\" :key=\"trans.id\" :class=\"{ 'high-spending': trans.amount > 100 }\">\n                <td>{{ trans.date }}</td>\n                <td>{{ trans.description }}</td>\n                <td>{{ trans.amount }}</td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </div>\n    </section>\n    <section class=\"financial-goals\">\n      <h2>Your Financial Goals</h2>\n      <div class=\"goals\">\n        <div class=\"goal\" :style=\"goalStyle(50)\" @click=\"showGoalProgress('Home Renovation', 50)\">\n          <i class=\"fas fa-home\"></i>\n          <p>Home Renovation<br>300-350 million VND</p>\n        </div>\n        <div class=\"goal\" :style=\"goalStyle(30)\" @click=\"showGoalProgress('New Sofa', 30)\">\n          <i class=\"fas fa-couch\"></i>\n          <p>New Sofa<br>56-75 million VND</p>\n        </div>\n        <div class=\"goal\" :style=\"goalStyle(70)\" @click=\"showGoalProgress('Buy a Tesla', 70)\">\n          <i class=\"fas fa-car\"></i>\n          <p>Buy a Tesla<br>1.9 - 2 billion VND</p>\n        </div>\n      </div>\n    </section>\n    <div v-if=\"showModal\" class=\"modal\" @click=\"showModal = false\">\n      <div class=\"modal-content\" @click.stop>\n        <h3>{{ goalTitle }}</h3>\n        <p>